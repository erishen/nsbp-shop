{
  "name": "nsbp-shop",
  "version": "1.0.0",
  "description": "NSBP project: nsbp-shop",
  "main": "index.js",
  "homepage": "https://nsbp.erishen.cn",
  "scripts": {
    "dev": "npm-run-all -l -s dev:init -p dev:build:*",
    "dev:init": "cross-env INIT=1 webpack --config config/webpack.server.js --mode development",
    "dev:build:server": "webpack --config config/webpack.server.js --mode development --watch",
    "dev:build:client": "webpack --config config/webpack.client.js --mode development --watch",
    "dev:build:start": "nodemon --watch build --ext js --exec \"node ./scripts/start.js\"",
    "build": "pnpm run clean && npm-run-all -l -p build:server build:client",
    "build:server": "webpack --config config/webpack.server.js --mode production",
    "build:client": "webpack --config config/webpack.client.js --mode production",
    "start": "node ./scripts/start.js",
    "clean": "rimraf build && rimraf public/js && rimraf public/css && rimraf public/client.* && rimraf public/*.js && rimraf public/*.js.map && rimraf public/*.txt && rimraf public/*.json && rimraf config/.temp_cache",
    "convert:webp": "node scripts/convert-to-webp.js",
    "generate:responsive": "node scripts/generate-responsive-images.js",
    "build:prod": "pnpm run generate:responsive && pnpm run build",
    "format": "prettier --write **/*.{js,css,less,scss,ts,tsx}",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "prepare": "husky"
  },
  "keywords": [
    "react",
    "ssr",
    "webpack",
    "less",
    "scss",
    "sass",
    "styled-components"
  ],
  "author": "Erishen Sun",
  "license": "MIT",
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "@redux-devtools/extension": "^3.3.0",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.7.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.0",
    "framer-motion": "^12.25.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "probe-image-size": "^7.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet": "^6.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.12.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.80.0",
    "serialize-javascript": "^7.0.2",
    "styled-components": "^6.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.25.0",
    "@babel/plugin-transform-optional-chaining": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.25.0",
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.0.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.0",
    "@playwright/test": "^1.44.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/loadable__component": "^5.13.3",
    "@types/loadable__server": "^5.12.5",
    "@types/lodash": "^4.17.15",
    "@types/probe-image-size": "^7.0.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.10",
    "@types/serialize-javascript": "^5.0.3",
    "@types/styled-components": "^5.1.36",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "browser-sync": "^3.0.4",
    "browser-sync-webpack-plugin": "^2.3.0",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^10.1.0",
    "cross-spawn": "^7.0.6",
    "css-loader": "^7.1.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^17.2.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^17.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "less": "^4.2.0",
    "less-loader": "^12.0.0",
    "lint-staged": "^15.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^10.6.1",
    "prettier": "^3.3.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^6.1.2",
    "sass-loader": "^16.0.0",
    "sharp": "^0.34.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.0",
    "typescript": "^5.8.3",
    "typescript-loadable-components-plugin": "^1.0.2",
    "webpack": "^5.96.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss}": [
      "prettier --write"
    ]
  }
}